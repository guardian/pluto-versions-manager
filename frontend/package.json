{
  "name": "pluto-versions-manager",
  "version": "1.0.0",
  "description": "Frontend to check the running versions of prexit components against what is available",
  "main": "index.tsx",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf node_modules ../public/javascripts/bundle.js",
    "snyk-protect": "snyk protect",
    "lint": "prettier --check app __tests__",
    "lint:fix": "prettier --write app __tests__"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "tsx",
      "ts",
      "json"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest",
      "node_modules/react-multistep/.+\\.js$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-multistep)"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./jestSetup.jsx"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/react-helmet": "^6.1.0",
    "bl": "4.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": ">=4",
    "ini": "1.3.6",
    "jest": "^26.0.1",
    "jest-cli": "^24.8.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-junit": "^4.0.0",
    "lint-staged": ">=10",
    "moxios": "^0.4.0",
    "object.entries": "^1.1.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "sinon": "^8.0.4",
    "source-map-loader": "^1.0.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^7.0.5",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/date-fns": "^1.3.6",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.21.1",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.2.4",
    "date-fns": "^2.16.1",
    "identity-obj-proxy": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash.omit": "^4.5.0",
    "pluto-headers": "https://gitlab.com/codmill/customer-projects/guardian/pluto-headers.git#v1.3.6",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "raven-js": "^3.24.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "snyk": "^1.518.0",
    "stream-browserify": "^3.0.0",
    "typescript-loader": "^1.1.3",
    "ua-parser-js": "^0.7.24",
    "util": "^0.12.3",
    "webpack": "^5.26.0",
    "webpack-cli": "^4.5.0",
    "y18n": "^4.0.1",
    "yarn": "^1.21.1"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
